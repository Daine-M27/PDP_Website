mixin sheetFour(options)
  text()
    tspan(x='1260.3188' y='196.42857' style='stroke-width:1.04167')
  rect(x='1445.32' y='52.8782' width='168.548' height='25.8553' style='fill:none;stroke-width:.520833;stroke:#000000')
  text()
    tspan(x='1462.6995' y='70.780205' style='font-family:Arial;font-size:11.1111px;stroke-width:1.04167') OUTLET IDENTIFICATION
  
  text()
    tspan(x='115' y='872.89709' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') NOTES:
  text()
    tspan(x='115' y='896.95972' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') 1.&nbsp;&nbsp;&nbsp;CIRCUITS ARE NUMBERED FROM FEED END (SEE SHEET 3).
  text()
    tspan(x='115' y='915.72321' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') 2.&nbsp;&nbsp;&nbsp;DEFAULT CIRCUIT WIRING IS SHOWN IN CHART. FOR MORE THAN 1 CIRCUIT CUSTOMER CAN
    tspan(x='115' y='933.08435' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;SPECIFY CUSTOM CIRCUIT WIRING, OTHERWISE THE DEFAULT DIRCUIT WIRING WILL BE USED.
  text()
    tspan(x='115' y='951.56299' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') 3.&nbsp;&nbsp;&nbsp;DEFAULT OUTLET LABELING IS SHOWN IN CHART.  CUSTOMER CAN SPECIFY CUSTOM
    tspan(x='115' y='968.92413' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;OUTLET LABELING, OTHERWISE THE DEFAULT OUTLET LEBELING WILL BE USED.  CUSTOM
    tspan(x='115' y='986.28522' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;OUTLET LABELING IS LIMITED TO 8 CHARACTERS.
  text()
    tspan(x='115' y='1002.4852' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') 4.&nbsp;&nbsp;&nbsp;PLEASE FILL IN THE CHART AND RETURN TO THE LIGHT SOURCE.  MANUFACTURING LEAD TIME 
    tspan(x='115' y='1019.8463' style='font-family:sans-serif;font-size:13.8889px;stroke-width:1.04167') &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;WILL NOT START UNTIL CUSTOM OR DEAULT LABELING HAS BEEN CONFIRMED.
            
            //----------------column data for chart---------------------//
            //-3rd column
            //- rect(x='1114.65' y='116.171' width='474.1' height='26.1' style='fill:none;stroke-width:.899999;stroke:#000000')
            //-2nd column or center column
            //- rect(x='612.95' y='116.171' width='474.1' height='26.1' style='fill:none;stroke-width:.899999;stroke:#000000')
            //-1st column
            //- rect(x='111.245' y='116.171' width='474.1' height='26.1' style='fill:none;stroke-width:.9;stroke:#000000')

            //-right column
            //- rect(x='865.595' y='116.171' width='474.1' height='26.1' style='fill:none;stroke-width:.9;stroke:#000000') 
            //-left column
            //- rect(x='360.306' y='116.171' width='474.1' height='26.1' style='fill:none;stroke-width:.9;stroke:#000000')

  //--------------------------------------------setup tables for custom labels--------------------------------------------//
  - var columnXaxis = []
  - var outlets = parseInt(options.numberOfOutlets)
  - var maxCir = parseInt(options.numberOfCircuits)
    //- function returns the remainder of max circuit divided by input
  - function circuit(x, last){ return x % last + 1 }

  if outlets <= 25
    - columnXaxis = [ '612.95' ]
  if (outlets > 25 && outlets <= 50)
    - columnXaxis = [ '360.306', '865.595' ]
  if (outlets >= 51)
    - columnXaxis = [ '111.245', '612.95', '1114.65' ]  
    
  each x, index in columnXaxis 
    g(transform=`translate(${x} 116.171)`)
      path(d='m350.099 26.55h124.451v25h-124.451zm-124.451-2e-5h124.451v25h-124.451zm-75.0662 0h75.0662v25h-75.0662zm-75.0662 0h75.0662v25h-75.0662zm-75.0662 0h75.0662v25h-75.0662z' style='fill:none;stroke-width:.9;stroke:#000000')
      text()
        tspan(x='129.69969' y='18.044098' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') CIRCUIT AND LABEL IDENTIFICTION
      text()
        tspan(x='1260.3188' y='196.42857' style='stroke-width:1.04167')    
      rect(x='.449997' y='.449997' width='474.1' height='26.1' style='fill:none;stroke-width:.9;stroke:#000000')
      text()
        tspan(x='8.1491165' y='43.594101' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') POSITION
      text()
        tspan(x='85.025017' y='37.992416' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') DEFAULT
      text()
        tspan(x='87.518303' y='49.006561' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') CIRCUIT
      text()
        tspan(x='234.00742' y='43.581894' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') SPECIFY CIRCUIT
      text()
        tspan(x='160.09125' y='37.992416' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') DEFAULT
      text()
        tspan(x='168.42255' y='49.018768' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') LABEL
      text()
        tspan(x='364.07333' y='43.594116' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') CUSTOM LABEL
      
      //-row of chart  // translate y * 25 for new row            
      - for(var r = 0; r < options.outletPositions[index].length ; r++)
        - var position = options.outletPositions[index][r]
          //- track outlet positions with custom lables and circuits
        - var customCircuit = options.customLabels[(position - 1)].specifyCircuit
        - var customLabel = options.customLabels[(position - 1)].customLabel
        g(transform=`translate(0 ${r * 25})`)       
          path(d='m350.099 51.55h124.451v25h-124.451zm-124.451-2e-5h124.451v25h-124.451zm-75.0662 0h75.0662v25h-75.0662zm-75.0662 0h75.0662v25h-75.0662zm-75.0662 0h75.0662v25h-75.0662z' style='fill:none;stroke-width:.9;stroke:#000000')
          //-position text
          text(text-anchor='middle')
            tspan(x='37' y='68.594101' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') !{position}
          //-default circuit label text
          text(text-anchor='middle')
            tspan(x='113' y='68.594101' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') #{circuit(r, maxCir)}
          //-default label text
          text(text-anchor='middle')
            tspan(x='187.5' y='68.594101' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') C#{circuit(r, maxCir)}
          //-specify circuit text
          text(text-anchor='middle')
            tspan(x='288' y='68.594101' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') !{customCircuit}
          //-custom label text
          text(text-anchor='middle')
            tspan(x='412' y='68.594101' style='font-family:Arial;font-size:12.5px;stroke-width:1.04167') !{customLabel}
  