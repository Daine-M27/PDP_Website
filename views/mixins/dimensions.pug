  //---------------------- logic for dimensions mixins ----------------------//
  //- Power Input
  +outletSpacing()
  - if (options.powerInput === 'L' || options.powerInput === 'W')
    +leadWhip()
  - else if (options.powerInput === 'S')
    +s19()

  //- Pipe Length
  - if (options.pipeLength === '48')
    +48Inch(options.endCap, options.powerInput, options.powerInputPosition)  
  - else if (options.pipeLength === '64')
    +64Inch(options.endCap, options.powerInput, options.powerInputPosition)  
  - else if (options.pipeLength === '80')
    +80Inch(options.endCap, options.powerInput, options.powerInputPosition)  
  - else if (options.pipeLength === '96')
    +96Inch(options.endCap, options.powerInput, options.powerInputPosition)  
  - else if (options.pipeLength > 96)
    +96InchPlus(options.endCap, options.powerInput, options.powerInputPosition)

  //- DMX Universe
  - if (options.dmxUniverses === '1')
    +singleDMXBackView()
    +DMXSide(0)
  - else if (options.dmxUniverses === '2')
    +dualDMXBackView()
    +DMXSide(77)